class Solution(object):
    def calculate(self, s):
        """
        :type s: str
        :rtype: int
        """
        num = 0
        prevOp = '+'
        s = s.replace(" ", "")
        stack = []

        for i in range(len(s)):
            c = s[i]

            if c.isdigit():
                num = num * 10 + int(c)

            if not c.isdigit() or i == len(s) - 1:
                if prevOp == '+':
                    stack.append(num)
                elif prevOp == '-':
                    stack.append(-num)
                elif prevOp == '*':
                    stack.append(stack.pop() * num)
                elif prevOp == '/':
                    stack.append(math.trunc(stack.pop() / float(num))) ## Why we dont use python

                prevOp = c
                num = 0  # Reset number after operation

        return sum(stack)
